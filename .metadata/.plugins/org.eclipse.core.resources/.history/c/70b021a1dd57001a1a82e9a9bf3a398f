#include<stdio.h>
#include<math.h>

int main(void){
	double x1;
	double x2;
	double y1;
	double y2;
	double distance_x;
	double distance_y;
	double distance;

	distance_x = x2 - x1;
	distance_y = y2 - y1;
	distance = sqrt (distance_x * 2)+(distance_y * 2);
	printf("veuillez entrer deux coordonnées ");
	scanf("%d %d",&x1,&y1);
	printf("veuillez entrer encore deux autres coordonnnées");
	scanf("%d %d",&x2,&y2);

	printf("la distance est de %d",distance);






	return 0;
}
